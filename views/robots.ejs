<!DOCTYPE html>
<html>
 <head>
 <% include ./partials/head %>
 </head>
 <body>
   <header>
     <% include ./partials/navbar %>
   </header>
   <h1 class='center'>Robots</h1>
   <hr />
   <div class='row'>
     <div class='col s12 m4'>
       <h3>Add Robot</h3>
       <%#The action is where it goes and the action is how it gets there %>
       <form action='/robots' method='post'>
         <input type='text' name='name' placeholder='name' />
         <button class='btn btn-waves' type='submit'>Add Robots</button>
       </form>
     </div>
     <table class='table col m8 s12'>
       <thead>
         <th>Title</th>
         <th>x</th>
       </thead>
       <tbody>
         <% robots.forEach( function(robot) { %>
           <% var robotClean = robot.replace(/ /g, "%20") %>
           <% var url='/robots/delete/' + robotClean %>
           <tr>
             <td><%= robot %></td>
             <td><a href=<%= url %> + <%= robot %></a>Delete</td>
         <% }); %>
       </tbody>
     </table>
   </body>
</html>
